<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EquationApp.MainPage"
             Title="Paguco">
  <MasterDetailPage.Master>
    <ContentPage Title="SideMenu" BackgroundColor="#FFFCF9" >
      <StackLayout Orientation="Vertical">
        <Label Text="paguco" HeightRequest="60" BackgroundColor="#FF4C3B" FontSize="25" TextColor="#FFF" YAlign="Center" />
        <Button Text="Home" Style="{StaticResource primaryNavButton}" Clicked="NavigateHomePage" />
        <Button Text="About" Style="{StaticResource primaryNavButton}" Clicked="NavigatePageAbout" />
      </StackLayout>
    </ContentPage>
  </MasterDetailPage.Master>
</MasterDetailPage>
